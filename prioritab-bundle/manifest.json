{
	"name": "Tasker Tab",
	"description": "A new tab page where you can add tasks and look at the time left",
	"version": "1.0.0",
	"chrome_url_overrides": {
		"newtab": "prioritab.html"
	},
	"icons": {
		"16": "prioritab_logo_v2_16.png",
		"32": "prioritab_logo_v2_32.png",
		"48": "prioritab_logo_v2_48.png",
		"128": "prioritab_logo_v2_128.png"
	},
	"manifest_version": 2,
	"permissions": ["storage"],
	"content_security_policy": "script-src 'self' https://code.jquery.com/jquery-3.0.0.js https://code.jquery.com/jquery-migrate-3.3.2.js; object-src 'self'"
}
